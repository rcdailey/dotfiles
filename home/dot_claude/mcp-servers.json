{
  "exa": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "exa-mcp-server@latest",
      "enabledTools=[\"web_search_exa\",\"crawling_exa\"]"
    ],
    "env": {
      "EXA_API_KEY": "rbw:exa-api-key"
    }
  },
  "context7": {
    "type": "http",
    "url": "https://mcp.context7.com/mcp",
    "headers": {
      "CONTEXT7_API_KEY": "rbw:context7-api-key"
    }
  }
}
