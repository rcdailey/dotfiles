{
  "markitdown": {
    "type": "stdio",
    "command": "uvx",
    "args": ["markitdown-mcp"],
    "env": {}
  },
  "octocode": {
    "type": "stdio",
    "command": "npx",
    "args": ["-y", "octocode-mcp@latest"],
    "env": {}
  },
  "tavily": {
    "type": "stdio",
    "command": "npx",
    "args": ["-y", "tavily-mcp@latest"],
    "env": {
      "TAVILY_API_KEY": "rbw:tavily-api-key"
    }
  },
  "sequential-thinking": {
    "type": "stdio",
    "command": "npx",
    "args": ["-y", "@modelcontextprotocol/server-sequential-thinking@latest"],
    "env": {}
  }
}
